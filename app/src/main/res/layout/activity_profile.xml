<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileVM" type="com.ikramsapplication.app.modules.profile.data.viewmodel.ProfileVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackarrowleft"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <FrameLayout android:id="@+id/framePROFILE"
            android:layout_gravity="start"
            android:layout_height="match_parent" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearColumnarrowleft"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle_1">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_34pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_marginBottom="@dimen/_115pxv"
                        android:layout_width="@dimen/_27pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>

                    <ImageView
                        android:id="@+id/imageEllipseTen"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse1}"
                        android:layout_width="@dimen/_128pxh"
                        android:layout_height="@dimen/_128pxh"
                        android:layout_marginStart="@dimen/_81pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:scaleType="centerCrop"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_ellipse1" />

                    <ImageView android:id="@+id/imageCheckmark"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginBottom="@dimen/_116pxv"
                        android:layout_marginStart="@dimen/_84pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_checkmark"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtGroupSix"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_35pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_40pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_5pxv"
                    android:paddingStart="@dimen/_27pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@{profileVM.profileModel.txtGroupSix}"
                    style="@style/txtRoundedOutline" tools:text="@string/lbl_name"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnUpload"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginEnd="@dimen/_35pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_154pxh"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_upload"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_3" tools:text="@string/lbl_upload"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowrectanglethirtythree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_35pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_50pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/imageRectangleThirtyThree"
                        android:layout_height="@dimen/_177pxh"
                        android:layout_width="@dimen/_167pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle33}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle33"/>
                    <ImageView
                        android:id="@+id/imageRectangleThirtyFour"
                        android:layout_height="@dimen/_177pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="@dimen/_167pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle33}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle33"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowrectanglethirtyfive"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_119pxv"
                    android:layout_marginEnd="@dimen/_35pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/imageRectangleThirtyFive"
                        android:layout_height="@dimen/_177pxh"
                        android:layout_width="@dimen/_167pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle33}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle33"/>
                    <ImageView android:id="@+id/imageRectangleThirtySix"
                        android:layout_height="@dimen/_177pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="@dimen/_167pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle33}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle33"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageRectangleThirtySeven"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_177pxh"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_167pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_10pxh}"
                imageRes="@{@drawable/img_rectangle33}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle33"/>
            <ImageView android:id="@+id/imageRectangleThirtyEight"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_177pxh"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_167pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_10pxh}"
                imageRes="@{@drawable/img_rectangle33}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle33"/>
        </FrameLayout>
    </FrameLayout>
</layout>
