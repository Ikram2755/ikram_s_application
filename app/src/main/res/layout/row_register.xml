<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="registerRowModel" type="com.ikramsapplication.app.modules.register.data.model.RegisterRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowvector"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <LinearLayout android:id="@+id/linearRowsettings"
            android:layout_height="wrap_content"
            android:layout_weight="0.86" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageSettings"
                android:layout_height="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_25pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_settings"
                tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPassword"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_37pxh"
                android:layout_weight="0.76" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{registerRowModel.txtPassword}"
                style="@style/txtLoraromanmedium17" tools:text="@string/lbl_password"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageVectorOne"
            android:layout_height="@dimen/_16pxh"
            android:layout_marginBottom="@dimen/_3pxv"
            android:layout_marginStart="@dimen/_154pxh"
            android:layout_marginTop="@dimen/_6pxv"
            android:layout_width="@dimen/_23pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_vector_white_a700"
            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700"/>
    </LinearLayout>
</layout>
