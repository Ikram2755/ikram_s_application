<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="recomendedRowModel" type="com.ikramsapplication.app.modules.home.data.model.RecomendedRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowevent"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylebluegray_900cornerRadius">
        <ImageView android:id="@+id/imageRowimage"
            android:layout_height="@dimen/_54pxh"
            android:layout_marginBottom="@dimen/_5pxv"
            android:layout_marginStart="@dimen/_4pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="@dimen/_66pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_5a1c82c9164b70}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_5a1c82c9164b70"/>
        <FrameLayout android:id="@+id/frameStackinfoOne"
            android:layout_height="@dimen/_47pxv"
            android:layout_marginBottom="@dimen/_12pxv"
            android:layout_marginEnd="@dimen/_4pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_239pxh">
            <LinearLayout android:id="@+id/linearRowclose"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackclose"
                    android:layout_height="@dimen/_39pxv" android:layout_width="@dimen/_128pxh">
                    <ImageView android:id="@+id/imageCloseTwo"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                    <LinearLayout android:id="@+id/linearRowcloseOne"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_18pxh"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageCloseOneTwo"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_close"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                        <ImageView android:id="@+id/imageCloseTwoTwo"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_close"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                        <ImageView android:id="@+id/imageInfoTwo"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_width="@dimen/_17pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_info_orange_a700_16x17"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_info_orange_a700_16x17"/>
                        <ImageView android:id="@+id/imageCloseThreeTwo"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_close_orange_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_close_orange_a700"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtChocolateCake"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{recomendedRowModel.txtChocolateCake}"
                        style="@style/txtLoraromanbold16" tools:text="@string/lbl_chocolate_cake"/>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearRowinfoOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_68pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_weight="0.25"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageInfoOneTwo"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_info"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_info"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTimeTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.65"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{recomendedRowModel.txtTimeTwo}"
                        style="@style/txtLoraromansemibold9_2" tools:text="@string/lbl_10_03"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageLocationTwo"
                android:layout_gravity="top|end"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="@dimen/_22pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_location"
                tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
        </FrameLayout>
    </LinearLayout>
</layout>
