<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="recommendedRowModel" type="com.ikramsapplication.app.modules.recommended.data.model.RecommendedRowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnlocation"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical" style="@style/groupStylegray_600_66cornerRadius">
        <LinearLayout android:id="@+id/linearRowlocation"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageLocationSix"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginBottom="@dimen/_56pxv"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="@dimen/_22pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_location"
                tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
            <ImageView android:id="@+id/imagePngwingSixThree"
                android:layout_height="@dimen/_87pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_width="@dimen/_99pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_24pxh}"
                imageRes="@{@drawable/img_pngwing6}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_pngwing6"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtBreakfastThree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{recommendedRowModel.txtBreakfastThree}"
            style="@style/txtLoraromansemibold9" tools:text="@string/lbl_breakfast"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtToastwithBerrThree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{recommendedRowModel.txtToastwithBerrThree}"
            style="@style/txtLoraromansemibold9_1" tools:text="@string/msg_toast_with_berr"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowinfo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageInfoSix"
                android:layout_height="@dimen/_16pxh"
                android:layout_width="@dimen/_13pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_info"
                tools:ignore="ContentDescription" tools:src="@drawable/img_info"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTimeSix"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.69" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{recommendedRowModel.txtTimeSix}"
                style="@style/txtLoraromansemibold9_2" tools:text="@string/lbl_10_03"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowclose"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_23pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageCloseSix"
                android:layout_height="@dimen/_15pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
            <ImageView android:id="@+id/imageCloseOneSix"
                android:layout_height="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
            <ImageView android:id="@+id/imageCloseTwoSix"
                android:layout_height="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
            <ImageView android:id="@+id/imageCloseThreeSix"
                android:layout_height="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_15pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
            <ImageView android:id="@+id/imageInfoOneSix"
                android:layout_height="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_width="@dimen/_17pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_info_orange_a700"
                tools:ignore="ContentDescription" tools:src="@drawable/img_info_orange_a700"/>
        </LinearLayout>
    </LinearLayout>
</layout>
